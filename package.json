{
  "name": "rag_pip",
  "version": "1.0.0",
  "description": "This RAg will answer from pdf",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/core/server.js",
    "frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm start\" \"npm run frontend\"",
    "dev:backend": "npm start",
    "dev:frontend": "npm run frontend",
    "ingest": "node src/processing/ingest.js",
    "test-ingest": "node src/processing/test_ingestion.js",
    "test": "node tests/test-suite.js",
    "test:verbose": "node tests/test-suite.js --verbose",
    "test:unit": "node tests/test-suite.js --unit",
    "test:integration": "node tests/test-suite.js --integration",
    "test:performance": "node tests/test-suite.js --performance",
    "test:load": "node tests/test-suite.js --load",
    "test:report": "node tests/test-suite.js --report",
    "test:quick": "node test-runner.js --quick",
    "test:full": "node test-runner.js --full",
    "chromadb:start": "docker run -d --name chromadb -p 8000:8000 chromadb/chroma",
    "chromadb:stop": "docker stop chromadb && docker rm chromadb",
    "chromadb:status": "docker ps | grep chromadb || echo 'ChromaDB is not running'",
    "chromadb:logs": "docker logs chromadb",
    "chromadb:test": "node -e \"import { ChromaClient } from 'chromadb'; const client = new ChromaClient({ host: 'localhost', port: 8000 }); const collection = await client.getOrCreateCollection({ name: 'test_collection' }); console.log('âœ… ChromaDB connection successful! Documents:', await collection.count());\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@huggingface/inference": "^4.8.0",
    "@langchain/community": "^0.3.55",
    "@langchain/core": "^0.3.75",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/textsplitters": "^0.0.1",
    "axios": "^1.12.1",
    "chromadb": "*",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "langsmith": "^0.3.67",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
